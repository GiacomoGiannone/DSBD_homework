# Copy this file to .env and fill values locally. Do NOT commit .env

# Alert Notifier SMTP configuration
DISABLE_EMAIL=false
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@example.com
SMTP_PASS=your_app_password_or_smtp_password
SMTP_FROM=your_email@example.com

# OpenSky client credentials (used to obtain OAuth token)
CLIENT_ID=your_client_id
CLIENT_SECRET=your_client_secret

# Database credentials for Kubernetes Secrets
MYSQL_ROOT_PASSWORD=root
DB_USER=root
DB_PASSWORD=root